<p style="color: green"><%= notice %></p>

<%= render @score %>

<div>
  <% if current_user_is_admin? %>
    <%= link_to "Edit score", edit_score_path(@score), class: 'btn btn-primary me-1' %> 
  <% end %> 
  <%= link_to "Back to scores", scores_path, class: 'btn btn-secondary me-1' %>
  <%= button_to "Destroy score", @score, method: :delete, class: 'btn btn-danger' %>
</div>
