{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@babel/core": "^7.18.0",
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.2",
    "@popperjs/core": "^2.11.5",
    "babelify": "^10.0.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.2",
    "browserify": "^17.0.0",
